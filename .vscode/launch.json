  "version": "0.2.0",
  "configurations": [
    {
      "type": "aws-sam",
      "request": "direct-invoke",
      "name": "monitoring-ui:src/services/bffApi/websocket.initSocket (nodejs16.x)",
      "invokeTarget": {
        "target": "code",
        "projectRoot": "${workspaceFolder}/",
        "lambdaHandler": "src/services/bffApi/websocket.initSocket"
      },
      "lambda": {
        "runtime": "nodejs16.x",
        "payload": {},
        "environmentVariables": {}
      }
    },
    {
      "type": "pwa-chrome",
      "request": "launch",
      "name": "Launch Chrome against localhost",
      "url": "http://localhost:3000",
      "webRoot": "${workspaceFolder}"
    }
  ]
}